        <activity
            android:name=".Activities.AdminHomeActivity"
            android:exported="false" />

        <activity
            android:name=".Activities.BookingActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.Loginscreen"
            android:exported="false" />
        <activity
            android:name=".Activities.Userhomescreen"
            android:exported="false" />
        <activity
            android:name=".Activities.UserSignup"
            android:exported="false" />
        <activity
            android:name=".Activities.MainActivity"
            android:exported="false" />
        <activity
            android:name=".Activities.firstscreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>